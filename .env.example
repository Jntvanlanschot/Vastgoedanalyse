# Authentication
AUTH_USERNAME=your-username-here
AUTH_PASSWORD=your-password-here
AUTH_SECRET=your-super-secret-jwt-signing-key-that-should-be-at-least-32-characters-long-for-security

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Apify API
APIFY_API_TOKEN=your-apify-api-token-here

# Base URL (optional, defaults to localhost:3000)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Vector Tiles (optional)
NEXT_PUBLIC_USE_VECTORGRID=false
NEXT_PUBLIC_VTILES_URL=https://tiles.openfreemap.org/omt/{z}/{x}/{y}.pbf

# Environment
NODE_ENV=development
